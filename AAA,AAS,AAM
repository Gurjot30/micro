//AAA
SUB AH, AH
MOV AL, '6'
ADD AL, '7'
AAA
OR AL,30H
HLT

//AAS
SUB AH, AH
MOV AL,’9’
MOV BL,’3’
AAS
OR AL,30H
HLT

//AAM

MOV AL, '3'
ADD AL, '9'
AND AL,0FH
AND BL,0FH
MUL BL
AAM
OR AL,30H
HLT

//AAD
MOV AX,0207H
MOV BL,05H
AAD
DIV BL
HLT

//DAA
MOV AL,71H
ADD AL,43H
DAA
HLT

//DAS
MOV AL,71H
ADD AL,43H
DAS
HLT
